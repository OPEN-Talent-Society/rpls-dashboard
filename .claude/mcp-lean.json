{
  "mcpServers": {
    "claude-flow": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "agentic-flow", "mcp", "start"]
    },
    "supabase": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "dlx",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "${SUPABASE_ACCESS_TOKEN}",
        "--read-only",
        "false"
      ]
    },
    "context7": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "@upstash/context7-mcp@latest"]
    },
    "digitalocean-mcp": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "dlx",
        "@digitalocean/mcp@latest",
        "--services",
        "droplets,apps"
      ],
      "env": {
        "DIGITALOCEAN_API_TOKEN": "${DIGITALOCEAN_API_TOKEN}"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "@playwright/mcp@latest"]
    },
    "zai-mcp-server": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "@z_ai/mcp-server"],
      "env": {
        "Z_AI_API_KEY": "${Z_AI_VISION_KEY}",
        "Z_AI_MODE": "ZAI"
      }
    },
    "nocodb-base-ops": {
      "type": "stdio",
      "command": "pnpm",
      "args": [
        "dlx",
        "mcp-remote",
        "${NOCODB_URL}/mcp/ncmvk15tvewrerlg",
        "--header",
        "xc-mcp-token: ${NOCODB_MCP_TOKEN}"
      ]
    },
    "cortex": {
      "type": "stdio",
      "command": "/Users/adamkovacs/Documents/codebuild/codex-sandbox/scripts/mcp/cortex-mcp.sh"
    },
    "codex-subagents": {
      "type": "stdio",
      "command": "node",
      "args": ["/Users/adamkovacs/Documents/codebuild/codex-subagents-mcp/dist/index.js"],
      "env": {
        "CODEX_SUBAGENTS_DIR": "/Users/adamkovacs/Documents/codebuild/codex-cli-subagents/agents"
      }
    },
    "svelte": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "@sveltejs/mcp"]
    },
    "vercel": {
      "type": "stdio",
      "command": "pnpm",
      "args": ["dlx", "mcp-remote", "https://mcp.vercel.com"]
    }
  }
}
