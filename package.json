{
  "name": "rpls-dashboard",
  "version": "0.1.0",
  "description": "Open Talent Society RPLS Dashboard - Labor Market Intelligence Platform",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "process-data": "python3 scripts/process_data.py",
    "test": "vitest"
  },
  "devDependencies": {
    "@sveltejs/adapter-vercel": "^5.4.3",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "^5.2.9",
    "@types/node": "^20.10.0",
    "autoprefixer": "^10.4.16",
    "csv-parse": "^6.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.4",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "chart.js": "^4.4.1",
    "neural-trader": "^2.6.3",
    "postgres": "^3.4.7"
  },
  "type": "module",
  "pnpm": {
    "onlyBuiltDependencies": [
      "agentdb",
      "agentic-flow",
      "better-sqlite3",
      "esbuild",
      "hnswlib-node",
      "neural-trader",
      "protobufjs",
      "sharp"
    ]
  }
}
